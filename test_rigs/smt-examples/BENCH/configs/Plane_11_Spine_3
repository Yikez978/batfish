interface Management1
!
!
interface Loopback0
 ip address 6.11.3.1 255.255.255.255
 no shutdown
!
!
ip as-path regex-mode string
ip community-list standard LEAK_COMMUNITY permit 8075:10400
!
ip prefix-list DEFAULT_IPV4 seq 5 permit 0.0.0.0/0
ip prefix-list IPV4_18_OR_LONGER seq 5 permit 0.0.0.0/0 ge 18
!
router bgp 60011
 router-id 6.11.3.1
 bgp bestpath as-path multipath-relax
 bgp log-neighbor-changes
 maximum-paths 64 ecmp 64
 neighbor 2.0.11.6 remote-as 50000
 neighbor 2.0.11.6 soft-reconfiguration inbound
 neighbor 2.0.11.6 send-community
 neighbor 2.1.11.6 remote-as 50001
 neighbor 2.1.11.6 soft-reconfiguration inbound
 neighbor 2.1.11.6 send-community
 !
 address-family ipv4
  neighbor 2.0.11.6 activate
  neighbor 2.1.11.6 activate
  network 6.11.3.1 mask 255.255.255.255
 !
!
no logging console
logging buffered 65535 informational
logging trap informational
logging format timestamp high-resolution
logging facility local1
!
!
ip routing
!
no ip routing vrf MGMT
!
ipv6 unicast-routing
!
ip http client source-interface Management1
ip tftp client source-interface Management1
ip icmp source-interface loopback0
!
!
